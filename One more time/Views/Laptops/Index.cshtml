@model IEnumerable<One_more_time.Models.Table.Laptop>

@{
    ViewData["Title"] = "Index";
}
<a class="nav-link text-dark" asp-area="" asp-controller="Laptops" asp-action="MostExpensiveLaptops">Most Expensive Laptops</a>
<a class="nav-link text-dark" asp-area="" asp-controller="Laptops" asp-action="CheapestLaptops">Cheapest Laptops</a>
<a class="nav-link text-dark" asp-area="" asp-controller="Laptops" asp-action="LaptopsInBudget">Laptops In Budget</a>
<a class="nav-link text-dark" asp-area="" asp-controller="Laptops" asp-action="CompareTwoLaptops">Compare Two Laptops</a>
<a class="nav-link text-dark" asp-area="" asp-controller="Laptops" asp-action="ViewLaptopsByBrand">View Laptops By Brand</a>
<a class="nav-link text-dark" asp-area="" asp-controller="Laptops" asp-action="ViewAllBrandsAndLaptops">View All Brands And Laptops</a>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Brand)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Year)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Img)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Model)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Brand.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Year)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Img)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
